//*
//STEP030  EXEC PGM=IEBGENER,COND=(0,LT,STEP010)
//*
//SYSPRINT DD SYSOUT=*
//*
//SYSUT1   DD DSN=USER.CUSTOMER.REPORT,DISP=SHR
//*
//SYSUT2   DD SYSOUT=*
//*
//SYSIN    DD DUMMY
//*
